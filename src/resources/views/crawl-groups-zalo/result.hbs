<div class="container" style="font-size: 18px;">
    <h3 class="text-center">Result</h3>
    <div class="row border-bottom mt-2 fs-2">
        <div class="col-sm-2 fs-2 font-weight-bold" style="font-size:24px;">
            No.
        </div>
        <div class="col-sm-4 fs-2 font-weight-bold" style="font-size:24px;">
            Group Name
        </div>
        <div class="col-sm-2 fs-2 font-weight-bold text-center" style="font-size:24px;">
            Quantity
        </div>
        <div class="col-sm-4 fs-2 font-weight-bold" style="font-size:24px;">
            Actions
        </div>
    </div>
    {{#each groupzs}}
    <div class="row border-bottom mt-2">
        <div class="fs-2 col-sm-2" style="font-size:24px18px;">{{@index}}</div>
        <div class="fs-2 col-sm-4 font-weight-bold" style="font-size:18px;">{{this.groupName}}</div>
        <div class="fs-2 col-sm-2" style="font-size:18px;">
            <p class=" text-black text-center">{{this.groupQuantity}}</p>
        </div>
        <div class="fs-2 col-sm-4" style="font-size:18px;">
            <a class="text-center text-decoration-none" href="">View group member</a>
        </div>
    </div>
    {{/each}}
    <div class="mt-5 d-flex justify-content-center text-center mb-5"><a href="javascript:history.back()"
            class="bg-danger p-2 border-0 text-white text-center text-decoration-none rounded">Back</a></div>
</div>